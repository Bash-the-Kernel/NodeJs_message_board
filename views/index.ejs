<!DOCTYPE html>
<html>
<head>
  <title><%= title %></title>
</head>
<body>
  <h1><%= title %></h1>
  <a href="/new">New Message</a>
  <% messages.forEach((message, index) => { %>
    <div>
      <h3><%= message.user %></h3>
      <p><%= message.text %></p>
      <p><%= message.added %></p>
      <a href="/message/<%= index %>">Open</a>
    </div>
  <% }) %>
</body>
</html>